{
  "presets": [
    "latest",
    "stage-1"
  ],
  "plugins": [
    "babel-macros",
    "transform-object-assign",
    "transform-decorators-legacy",
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ],
    [
      "jsx-pragmatic",
      {
        "module": "preact",
        "export": "h",
        "import": "h"
      }
    ]
  ]
}